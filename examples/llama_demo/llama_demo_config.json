{
  "schema_version": 1,
  "instrumentation": {
    "enable_op_timing": true,
    "enable_memory_tracking": true,
    "record_tensor_names": true,
    "max_events": 50000,
    "op_types_to_trace": [],
    "backends_to_trace": ["cpu", "metal"]
  },
  "output": {
    "filename": "llama_demo_trace.ggmlviz",
    "write_to_file": true,
    "flush_interval": 1024
  },
  "logging": {
    "level": "INFO",
    "timestamp": true,
    "thread_id": false,
    "prefix": "[LLAMA_DEMO]"
  },
  "ui": {
    "live_mode": true,
    "poll_interval_ms": 50,
    "max_live_events": 25000
  }
}