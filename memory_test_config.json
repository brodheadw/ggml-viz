{
  "cli": {
    "verbose": true,
    "live_mode": false,
    "port": 8080
  },
  "instrumentation": {
    "max_events": 10000,
    "enable_op_timing": true,
    "enable_memory_tracking": true,
    "output_file": "memory_test_trace.ggmlviz"
  },
  "logging": {
    "level": "DEBUG",
    "enable_timestamps": true,
    "enable_thread_id": false,
    "prefix": "[MEMORY_TEST]"
  }
}